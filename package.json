{
    "name": "ecommerce",
    "version": "3.0.0",
    "description": "ecommerce",
    "main": "index.js",
    "scripts": {
        "start": "webpack-dev-server",
        "clean": "rimraf dist_temp",
        "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
        "lint": "eslint ./src/**/**/**/**.{js,jsx}",
        "test": "jest",
        "test:watch": "jest --watch",
        "coverage": "jest --coverage",
        "webpack:dev": "webpack --colors",
        "webpack:prod": "webpack --config webpack.production.config.js --colors"
    },
    "author": "Kliment Petrov <kleopetroff@gmail.com>",
    "license": "MIT",
    "jest": {
        "setupFiles": [
            "<rootDir>/config/jest/shim.js"
        ],
        "collectCoverageFrom": [
            "app/**/*.{js,jsx,ts,tsx}"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/assetsTransformer.js",
            "\\.(css|less)$": "<rootDir>/config/jest/assetsTransformer.js"
        },
        "testMatch": [
            "**/?(*.)(spec|test).js?(x)"
        ],
        "transform": {
            "\\.js$": "babel-jest"
        }
    },
    "dependencies": {
        "@ckeditor/ckeditor5-build-balloon": "^11.1.1",
        "@ckeditor/ckeditor5-build-classic": "^11.0.1",
        "@ckeditor/ckeditor5-react": "^1.0.0-beta.1",
        "@material-ui/core": "^3.0.0",
        "@material-ui/icons": "^3.0.0",
        "@material-ui/lab": "latest",
        "axios": "^0.18.0",
        "bootstrap": "^4.1.3",
        "cross-env": "^5.2.0",
        "google-map-react": "^1.0.5",
        "history": "^4.7.2",
        "jwt-decode": "^2.2.0",
        "material-ui-pickers": "^1.0.0-rc.14",
        "moment": "^2.22.2",
        "react": "^16.4.1",
        "react-color": "^2.14.1",
        "react-custom-scrollbars": "^4.2.1",
        "react-datetime": "^2.15.0",
        "react-dom": "^16.4.1",
        "react-dropzone": "^4.2.13",
        "react-dropzone-component": "^3.2.0",
        "react-geosuggest": "^2.8.0",
        "react-google-login": "^3.2.1",
        "react-hot-loader": "^4.3.4",
        "react-image-crop": "^4.0.3",
        "react-infinite-scroller": "^1.2.0",
        "react-masonry-component": "^6.2.1",
        "react-modal": "^3.5.1",
        "react-photo-gallery": "^6.2.1",
        "react-player": "^1.6.6",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.3.1",
        "react-router-redux": "^5.0.0-alpha.9",
        "react-select": "^2.1.1",
        "react-share": "^2.2.0",
        "react-slick": "^0.23.1",
        "react-sortable-hoc": "^0.8.3",
        "reactstrap": "^6.2.0",
        "rebound": "^0.1.0",
        "redux": "^4.0.0",
        "redux-api-middleware": "^2.3.0",
        "redux-logger": "^3.0.6",
